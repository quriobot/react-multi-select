  "release": {
    "debug": false,
    "analyzeCommits": "./semantic_release/analyzeCommits",
    "verifyConditions": {
      "path": "./semantic_release/verifyConditions"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "multiselect",
    "form",
    "input",
    "listtolist",
