    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-junit": "^3.6.0",
    "jest-sandbox": "^1.1.2",
    "jsdom": "^11.3.0",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.9.1",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.8.2",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-addons-update": "^15.4.1",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "semantic-release": "^8.2.0",
    "storybook-readme": "3.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12",
    "webpack-merge": "^4.1.2",
    "webpack-strip": "^0.1.0"
