    "babel-preset-stage-2": "^6.24.1",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
